#ifndef LISTS_H
#define LISTS_H
/**
 *struct dlistint_t - doubly linked list
 *@n: node index
 *@prev: points to previous node
 *@next: points to next node
 *
 *Description: doubly linked list node structure
 */
typedef struct list_n
{
  int n;
  struct list_n *next;
  struct list_n *prev;
} dlistint_t;
size_t print_dlistint(const dlistint_t *h);

#endif
