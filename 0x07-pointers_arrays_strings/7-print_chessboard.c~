#include "main.h"
#include<string.h>
#include<stdio.h>
/**
 * print_chessboard - this function prints the chessboard
 * @a: the board
 *
 * Return: nothing
 */
void print_chessboard (char (*a) [8])
{
int i, k;
for (k = 0; k < 8; k++)
{
for (i = 0; i < 8; i++)
{
_putchar( a[k][i]);
}
_putchar('\n');
}
}
